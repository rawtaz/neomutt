msgid ""
"usage: neomutt [<options>] [-z] [-f <file> | -yZ]\n"
"       neomutt [<options>] [-Ex] [-Hi <file>] [-s <subj>] [-bc <addr>] [-a "
"<file> [...] --] <addr> [...]\n"
"       neomutt [<options>] [-x] [-s <subj>] [-bc <addr>] [-a <file> [...] "
"--] <addr> [...] < message\n"
"       neomutt [<options>] -p\n"
"       neomutt [<options>] -A <alias> [...]\n"
"       neomutt [<options>] -Q <query> [...]\n"
"       neomutt [<options>] -B\n"
"       neomutt [<options>] -D [-S]\n"
"       neomutt -v[v]\n"
